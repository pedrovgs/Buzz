{
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off"
    },
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "globals": { 
      "assert": true,
      "expect": true,
      "jasmine": true
    }
 
  }