{
  "name": "Buzz",
  "version": "0.0.1",
  "description": "A portable photo booth built on top of Electron, React and Raspberry Pi.",
  "main": "electron-starter.js",
  "homepage": "./",
  "repository": "git@github.com:pedrovgs/Buzz.git",
  "author": "Pedro Vicente Gómez Sánchez <pedrovicente.gomez@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "dev": "nf start -p 3000",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "buildForTests": "REACT_APP_RUNNING_TESTS=true yarn build",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "lint": "eslint src test",
    "fixLint": "eslint --fix src test",
    "test": "xvfb-maybe react-scripts test --env=jsdom",
    "verifyScreenshotTests": "yarn buildForTests && jest screenshotTests --runInBand --env=jsdom",
    "recordScreenshotTests": "yarn buildForTests && jest screenshotTests --runInBand --updateSnapshot --env=jsdom",
    "dist": "yarn distMac && yarn distRaspberry",
    "distMac": "yarn build && electron-packager . --platform=darwin --arch=x64 --out=dist --icon=assets/icons/logo.icns --overwrite",
    "distRaspberry": "yarn build && electron-packager . --platform=linux --arch=armv7l --out=dist --icon=assets/icons/logo.png --overwrite"
  },
  "dependencies": {
    "electron": "13.1.6",
    "firebase": "8.7.1",
    "firebase-tools": "9.16.0",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "moment": "^2.22.0",
    "monet": "^0.8.10",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-flexbox-grid": "^2.0.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1",
    "store": "^2.0.12",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "create-react-app": "^1.5.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "15.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "foreman": "^2.0.0",
    "image-size": "^0.6.2",
    "jest-image-snapshot": "^2.3.0",
    "jsverify": "^0.8.3",
    "prettier-eslint-cli": "^4.7.1",
    "react-scripts": "^1.1.1",
    "react-test-renderer": "^16.3.0",
    "redux-mock-store": "^1.5.1",
    "sharp": "0.28.3",
    "spectron": "^3.8.0",
    "webpack": "5.44.0",
    "webpack-dev-server": "4.0.0-beta.3",
    "xvfb-maybe": "^0.2.1"
  }
}
