{
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "globals": { 
      "assert": true,
      "expect": true
    }
 
  }